{
  "version": 2,
  "builds": [
    {
      "src": "**/*.html",
      "use": "@vercel/static"
    },
    {
      "src": "assets/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/assets/(?<path>.*)",
      "dest": "/assets/$path"
    },
    {
      "src": "^/(.*)",
      "dest": "/index.html"
    }
  ]
}
